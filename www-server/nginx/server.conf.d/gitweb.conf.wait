index index.html;

server {
	listen 10800;
	server_name gitweb.cqthink.com;

	location / {
		root /var/git/gitweb;
		index gitweb.cgi;
		include fastcgi_params;
		gzip off;
		fastcgi_param GITWEB_CONFIG /etc/gitweb.conf;

		if ($uri ~ "/gitweb.cgi") {
			fastcgi_pass unix:/run/fastcgi/fcgiwrap.socket;
		}
	}
}

